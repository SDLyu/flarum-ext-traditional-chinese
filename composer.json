{
    "name": "sdlyu/flarum-ext-traditional-chinese",
    "type": "flarum-extension",
    "description": "Traditional Chinese language pack for Flarum",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "SDLyu",
            "email": "dragon90321@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/SDLyu/flarum-ext-traditional-chinese.git"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
        "branch-alias": {
	    "dev-master": "0.1.x-dev"
	},
        "flarum-extension": {
            "title": "Chinese (zh_TW)",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "zh-tw",
            "title": "正體中文"
        }
    }
}
